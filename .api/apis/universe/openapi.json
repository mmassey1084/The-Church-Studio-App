              "type": "string",
              "default": "your_client_id"
            }
          },
          {
            "name": "response_type",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "default": "code"
            }
          },
          {
            "name": "redirect_uri",
            "in": "query",
            "description": "Your application's registered redirect URI",
            "required": true,
            "schema": {
              "type": "string",
              "default": "https://your_app.com/callback"
            }
          }
        ],
        "responses": {
